{
  "name": "obojobo-server",
  "version": "3.0.0-alpha-2",
  "private": true,
  "scripts": {
    "start": "yarn express:prod",
    "dev": "concurrently \"yarn express:dev\" \"yarn docengine:dev\"",
    "db:initdocker": "docker run --name db_postgres -d --restart=unless-stopped -p 5432:5432 postgres:9.6.1-alpine",
    "db:reset": "node node_modules/db-migrate/bin/db-migrate reset --config config/db.json",
    "db:migrateup": "node node_modules/db-migrate/bin/db-migrate up --config config/db.json",
    "db:migratedown": "node node_modules/db-migrate/bin/db-migrate down --config config/db.json",
    "db:seed": "node ./bin/seed_sample_draft.js",
    "express:dev": "pm2-dev start pm2_dev.yaml",
    "express:prod": "NODE_ENV=production node ./bin/www",
    "chunks:register": "node ./bin/register_obojobo_modules.js",
    "chunks:registerdev": "node ./bin/register_obojobo_modules.js --dev",
    "docengine:dev": "cd ./devsrc/obojobo-draft-document-engine && yarn dev",
    "docengine:setup": "\"yarn docengine:clone\" && \"yarn docengine:nodemodules\"",
    "docengine:clone": "git clone -b feature/new-data-model git@github.com:ucfcdl/Obojobo-Document-Engine.git devsrc/obojobo-draft-document-engine || echo 'already cloned'",
    "docengine:nodemodules": "cd ./devsrc/obojobo-draft-document-engine && yarn"
  },
  "engines": {
    "node": "^6.9.4"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "concurrently": "^3.1.0",
    "connect-pg-simple": "^3.1.2",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "debug": "~2.2.0",
    "eventemitter": "^0.3.3",
    "express": "~4.14.0",
    "express-ims-lti": "git+ssh://git@github.com/ucfcdl/express-ims-lti.git#ac7d1f4e9c5afa6470185d107b32ffdbaf28cd32",
    "express-session": "^1.15.1",
    "glob": "^7.1.1",
    "json-inflector": "^1.1.0",
    "moment": "^2.18.0",
    "morgan": "~1.7.0",
    "node-sass-middleware": "0.9.8",
    "obojobo-draft-document-engine": "git+ssh://git@github.com/ucfcdl/Obojobo-Document-Engine.git#feature/new-data-model",
    "pg-promise": "^5.5.0",
    "pm2": "^2.4.2",
    "pug": "^2.0.0-beta6",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {}
}
