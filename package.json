    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "backbone": "^1.3.3",
    "backbone.nativeajax": "^0.4.3",
    "bourbon": "^4.2.7",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "css-loader": "^0.28.1",
    "dragula": "^3.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "http-server": "^0.9.0",
