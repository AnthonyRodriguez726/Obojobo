    "whatwg-fetch": "^2.0.3"
  },
  "jest": {
    "testURL": "http://localhost:3000/view/00000000-0000-0000-0000-000000000000",
    "setupFiles": [
      "./setupTests.js"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "^Common(.*)$": "<rootDir>/src/scripts/common$1",
      "^Viewer(.*)$": "<rootDir>/src/scripts/viewer$1",
      "\\.scss$": "<rootDir>/__mocks__/ignore-file-stub.js",
      "\\.svg$": "<rootDir>/__mocks__/ignore-file-stub.js"
    },
    "collectCoverageFrom": [
