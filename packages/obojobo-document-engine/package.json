{
	"name": "obojobo-document-engine",
	"version": "3.4.0",
	"license": "AGPL-3.0-only",
	"description": "",
	"engines": {
		"yarn": "^1.0",
		"node": "^10.4.1"
	},
	"main": "index.js",
	"scripts": {
		"test": "TZ='America/New_York' jest --verbose",
		"test:ci":
			"TZ='America/New_York' CI=true jest --ci --useStderr --coverage --coverageReporters text-summary cobertura",
		"start": "webpack --watch",
		"dev": "webpack-dev-server --colors --port 8090 --host 0.0.0.0",
		"build": "webpack --mode production --optimize-minimize",
		"build:dev": "webpack --mode development",
		"build:minifysvg":
			"find ./ ! -path node_modules -type f -name '*.svg' ! -name '*.min.svg' -exec svgo {} --multipass --pretty --precision=2 \\;",
		"generateModuleManifest": "node ./bin/generate_module_manifest.js",
		"prettier:run": "prettier --write '{__{tests,mocks}__,server,src}/**/*.{js,scss}'",
		"prettier:debug": "prettier -l '{__{tests,mocks}__,server,src}/**/*.{js,scss}'",
		"precommit": "lint-staged && yarn lint:js && yarn test:ci",
		"lint": "yarn lint:js && yarn lint:css",
		"lint:js": "eslint .",
		"lint:css": "stylelint **/*.scss"
	},
	"prettier": {
		"printWidth": 100,
		"semi": false,
		"useTabs": true,
		"singleQuote": true
	},
	"browserslist": ["> 1%", "last 2 versions", "not dead"],
	"lint-staged": {
		"{__{tests,mocks}__,src,server}/**/*.{js,scss}": [
			"yarn prettier --write",
			"stylelint",
			"git add"
		]
	},
	"postcss": {
		"parser": "postcss-scss",
		"plugins": {}
	},
	"author": "",
	"dependencies": {
		"backbone": "^1.3.3",
		"date-fns": "^1.29.0",
		"immutable": "^3.8.2",
		"katex": "^0.9.0",
		"prop-types": "15.6.2",
		"react": "16.7.0",
		"react-dom": "16.7.0",
		"react-idle-timer": "^4.2.3",
		"react-transition-group": "^2.5.3",
		"slate": "^0.39.0",
		"slate-react": "^0.17.0",
		"slate-schema-violations": "^0.1.39",
		"underscore": "1.5.1",
		"url-parse": "^1.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-0",
		"@babel/polyfill": "7.0.0",
		"@babel/preset-env": "^7.2.0",
		"@babel/preset-react": "^7.0.0",
		"autoprefixer": "^9.4.2",
		"babel-core": "^7.0.0-bridge",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^23.6.0",
		"babel-loader": "^8.0.4",
		"babel-polyfill": "^6.26.0",
		"concurrently": "^3.5.0",
		"css-loader": "^2.0.0",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.8.0",
		"eslint": "^4.19.1",
		"eslint-plugin-react": "7.9.1",
		"husky": "^0.13.3",
		"jest": "23.4.2",
		"json-loader": "^0.5.4",
		"lint-staged": "^6.1.1",
		"mini-css-extract-plugin": "^0.5.0",
		"node-sass": "^4.9.0",
		"obojobo-document-engine": "*",
		"obojobo-document-xml-parser": "*",
		"postcss-loader": "^3.0.0",
		"prettier": "1.11.1",
		"react-test-renderer": "^16.7.0",
		"sass-loader": "^7.1.0",
		"smoothscroll-polyfill": "^0.3.4",
		"style-loader": "^0.21.0",
		"stylelint": "^9.2.0",
		"stylelint-config-standard": "^18.2.0",
		"stylelint-declaration-use-variable": "^1.7.0",
		"svg-url-loader": "^2.3.2",
		"webpack": "^4.12.0",
		"whatwg-fetch": "^3.0.0"
	},
	"jest": {
		"testURL": "http://localhost:3000/view/00000000-0000-0000-0000-000000000000/visit/1",
		"testMatch": ["**/__tests__/**/*.js?(x)"],
		"setupTestFrameworkScriptFile": "./setupTests.js",
		"verbose": false,
		"moduleNameMapper": {
			"^Common(.*)$": "<rootDir>/src/scripts/common$1",
			"^Viewer(.*)$": "<rootDir>/src/scripts/viewer$1",
			"\\.scss$": "<rootDir>/__mocks__/ignore-file-stub.js",
			"\\.svg$": "<rootDir>/__mocks__/ignore-file-stub.js"
		},
		"moduleDirectories": ["<rootDir>", "node_modules"],
		"collectCoverageFrom": ["src/scripts/**", "server/**"],
		"coverageReporters": ["text", "lcov"],
		"coveragePathIgnorePatterns": [
			"====THESE DONT NEED TESTS====",
			"<rootDir>/src/scripts/viewer.js",
			"<rootDir>/src/scripts/viewer/app.js",
			"<rootDir>/src/scripts/viewer/dist.js",
			"<rootDir>/src/scripts/oboeditor/index.js",
			"<rootDir>/src/scripts/oboeditor/app.js",
			"<rootDir>/src/scripts/oboeditor/dist.js",
			"<rootDir>/src/scripts/oboeditor/documents/",
			"<rootDir>/src/scripts/viewer/polyfills.js",
			".json",
			"====THESE ARE NOT USED RIGHT NOW====",
			"selection-handler.js",
			"<rootDir>/src/scripts/common/util/console.js",
			"====THESE ARE FOR THE EDITOR====",
			"command-handler.js",
			"<rootDir>/src/scripts/common/dist.js",
			"<rootDir>/src/scripts/common/chunk/util",
			"<rootDir>/src/scripts/common/models/chunk.js",
			"<rootDir>/src/scripts/common/chunk/text-chunk/text-group-el-util.js",
			"<rootDir>/src/scripts/common/text-group/text-group-cursor.js"
		],
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100,
				"statements": 100
			}
		}
	}
}
