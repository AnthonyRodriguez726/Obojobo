require './viewer-component.scss'

Common = window.ObojoboDraft.Common

OboComponent = Common.components.OboComponent
Button = Common.components.Button

ActionButton = React.createClass
	onClick: ->
		# console.clear()
		console.log @props.model.triggers
		@props.model.processTrigger 'onClick'
		# Viewer.util.AssessmentUtil.startAttempt Common.models.OboModel.models['assessment']

	render: ->
		`<OboComponent model={this.props.model}>
			<div className='obojobo-draft--chunks--action-button'>
				<Button onClick={this.onClick} value={this.props.model.modelState.label} />
			</div>
		</OboComponent>`

module.exports = ActionButton