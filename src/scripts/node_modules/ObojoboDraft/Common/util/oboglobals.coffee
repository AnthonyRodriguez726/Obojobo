GLOBAL_KEY = '__oboGlobals'

if not window[GLOBAL_KEY] then throw 'Unable to read Obo Globals - Must call after DOM load'

globals = new Map()
for key, value of window[GLOBAL_KEY]
	console.log('KEY IS', key, value)
	globals.set key, value

delete window[GLOBAL_KEY]

OboGlobals =
	get: (key) ->
		globals.get key

module.exports = OboGlobals