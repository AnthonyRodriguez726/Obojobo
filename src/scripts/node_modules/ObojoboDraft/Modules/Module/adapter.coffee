Adapter =
	construct: (model, attrs) ->
		if attrs?.content?.start?
			if attrs.content.start is 'unlimited'
				model.modelState.start = null
			else
				model.modelState.start = attrs.content.start

	clone: (model, clone) ->
		clone.modelState.start = model.modelState.start

	toJSON: (model, json) ->
		json.content.start = model.modelState.start


module.exports = Adapter