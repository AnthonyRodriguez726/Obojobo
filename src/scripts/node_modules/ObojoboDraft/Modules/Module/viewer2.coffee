ObojoboDraft = window.ObojoboDraft

OBO.register 'ObojoboDraft.Modules.Module', {
	type: 'module',
	default: true,
	adapter: require('./adapter'),
	componentClass: require('./viewer-component'),
	selectionHandler: null,
	getNavItem: (model) ->
		type: 'heading',
		label: model.title
		showChildren: true

	generateNav: (model) ->
		[{
			type: 'heading',
			label: model.title
		}]
}