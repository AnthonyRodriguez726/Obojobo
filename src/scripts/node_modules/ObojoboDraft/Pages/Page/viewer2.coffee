ObojoboDraft = window.ObojoboDraft
Dispatcher = ObojoboDraft.Common.flux.Dispatcher

OBO.register 'ObojoboDraft.Pages.Page', {
	type: 'page',
	default: true,
	componentClass: require('./viewer-component'),
	selectionHandler: null,
	getNavItem: (model) ->
		title = ''
		if model.title?
			title = model.title

		type: 'link',
		label: model.title,
		# path: ['page-' + (model.getIndex() + 1) + '-' + model.get('id')],
		path: [title.toLowerCase().replace(/ /g, '-')],
		showChildren: false
	# init: ->
