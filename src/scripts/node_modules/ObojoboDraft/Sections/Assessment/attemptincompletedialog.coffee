Dialog = window.ObojoboDraft.Common.components.modal.Dialog
ModalUtil = window.ObojoboDraft.Common.util.ModalUtil

module.exports = React.createClass
	onCancel: ->
		ModalUtil.hide()

	onSubmit: ->
		ModalUtil.hide()
		@props.onSubmit()

	render: ->
		`<Dialog width="32rem" buttons={[
			{
				value: 'Submit as incomplete',
				altAction: true,
				dangerous: true,
				onClick: this.onSubmit
			},
			{
				value: 'OK',
				onClick: this.onCancel,
				default: true
			}
		]}>
			<b>Wait! You left some questions blank.</b>
			<br />
			Finish answering all questions and submit again.
		</Dialog>`