emptyChar = require('ObojoboDraft/text/textconstants').EMPTY_CHAR

TextElement = React.createClass
	render: ->
		el = @props.descriptor

		# if el.nodeType is 'text'
		# 	return `<span>{this.props.descriptor.text || emptyChar}</span>`

		attrs = @props.descriptor.attrs
		attrs['data-group-index'] = @props.groupIndex

		React.createElement el.type, attrs, el.children.map( (child, index) ->
			switch child.nodeType
				when 'text'
					`<span key={index}>{child.text || emptyChar}</span>`
					# child.text || emptyChar
				else
					`<TextElement descriptor={child} key={index} />`
		)


module.exports = TextElement