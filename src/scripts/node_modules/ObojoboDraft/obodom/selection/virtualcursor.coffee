class VirtualCursor
	constructor: (@chunk, @data) ->

	isEquivalentTo: (otherCursor) ->
		if @chunk isnt otherCursor.chunk then return false
		@chunk.areCursorsEquivalent null, @data, otherCursor.data

	clone: ->
		# @chunk.cloneVirtualCaret @
		new VirtualCursor @chunk, Object.assign({}, @data)


module.exports = VirtualCursor