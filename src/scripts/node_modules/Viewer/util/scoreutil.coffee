Dispatcher = window.ObojoboDraft.Common.flux.Dispatcher

ScoreUtil =
	setScore: (id, score) ->
		Dispatcher.dispatch {
			type: 'score:set',
			value:
				id: id,
				score: score
		}

	clearScore: (id) ->
		Dispatcher.dispatch {
			type: 'score:clear',
			value:
				id: id
		}


module.exports = ScoreUtil